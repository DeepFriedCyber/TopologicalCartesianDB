# Vector Database Cloud Service Credentials
# Add your actual credentials here

# Weaviate Cloud Service
WEAVIATE_CLUSTER_URL=https://your-cluster.weaviate.network
WEAVIATE_API_KEY=your_weaviate_api_key_here

# Qdrant Cloud Service
QDRANT_CLUSTER_URL=https://your-cluster.qdrant.tech
QDRANT_API_KEY=your_qdrant_api_key_here

# Neon Database (if using vector extensions)
NEON_DATABASE_URL=postgresql://username:password@your-host.neon.tech/dbname?sslmode=require
NEON_API_KEY=your_neon_api_key_here

# Local development overrides (for testing)
LOCAL_QDRANT_URL=http://localhost:6333
LOCAL_WEAVIATE_URL=http://localhost:8080

# Benchmark configuration
BENCHMARK_DATASET=sift-small-en  # Options: sift-small-en, glove-25-angular, etc.
BENCHMARK_RESULTS_DIR=./benchmark_results
ENABLE_CLOUD_TESTING=true
